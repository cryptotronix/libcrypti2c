prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: @PACKAGE_NAME@
Description: Library for communicating Atmel's CryptoAuthentication devices.
Version: @PACKAGE_VERSION@
Libs.private: -lgcrypt
URL: @PACKAGE_URL@
Libs: -L${libdir} -lcryptoauth-@CRYPTOAUTH_API_VERSION@
Cflags: -I${includedir}/cryptoauth-@CRYPTOAUTH_API_VERSION@ -I${libdir}/cryptoauth-@CRYPTAUTH_API_VERSION@/include
